# Given a list lst and a number N, create a new list that contains each
# number of lst at most N times without reordering.

def delete_nth(order, max_e):
    lst = []
    for i in order:
        if lst.count(i) < max_e:
            lst.append(i)
    return lst




print(delete_nth([20,37,20,21], 1))
print(delete_nth([1,1,1,1,3,3,3,3,7,2,2,2,2], 3))
