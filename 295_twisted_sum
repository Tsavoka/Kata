# Find the sum of the digits of all the numbers from 1 to N (both ends
# included).

def compute_sum(n):
    return sum(map(int, ''.join(map(str, range(0, n+1)))))




print(compute_sum(1))   # 1
print(compute_sum(2))   # 3
print(compute_sum(3))   # 6
print(compute_sum(4))   # 10
print(compute_sum(10))  # 46
print(compute_sum(12))  # 51
